#include<stdio.h>
int main()
{
    int n,i,c=0;
    printf("enter the number :");
    scanf("%d",&n);
    for(i=2;i<=n;i++)
    {
        if(n%i==0)
        {
            c=c+1;
        }
    }
        if(c>1)
        {
            printf("the number is not prime");
        }
        else
        {
            printf("the number is  prime");
        }
    return 0;
}
