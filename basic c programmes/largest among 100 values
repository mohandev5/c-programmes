#include<stdio.h>
int main()
{
    int a[100],max,i,n,p;
    printf("enter no of elements :");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[1];
    for(i=1;i<=n;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
    }
    p=max-(max*0.10);
    printf("reduced amount among largest :%d",p);
    return 0;
}
