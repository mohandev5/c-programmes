#include<stdio.h>
int main()
{
    int n,rem,temp,rev=0;
    printf("enter the number :");
    scanf("%d",&n);
    temp=n;
    while(n!=0)
    {
        rem=n%10;
        rev=(rev*10)+rem;
        n=n/10;
    }
    if(rev==temp)
    {
      printf("the number is palindrome");  
    }
    else
    {
    printf("the number is not a palindrome");
    }
    return 0;
}
