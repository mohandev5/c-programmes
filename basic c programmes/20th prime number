#include<stdio.h>
int main()
{
    int n,i,x,c=0;
    for(n=2;c<=20;n++)
    {
        x=0;
        for(i=1;i<=n;i++)
        {
            if(n%i==0)
            x=x+1;
        }
        if(x==2)
        {
            c=c+1;
            if(c==20)
            break;
        }
    }
    printf("20th prime number :%d",n);
    return 0;
}
