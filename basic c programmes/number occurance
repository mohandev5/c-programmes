#include<stdio.h>
int main()
{
     long int n,rem,c=0,x;
    printf("enter the number :");
    scanf("%ld",&n);
    printf("enter the checking value :");
    scanf("%ld",&x);
    while(n>0)
    {
        rem=n%10;
        if(rem==x)
        c=c+1;
        n=n/10;
    }
    printf("number of occurance :%ld",c);
}
