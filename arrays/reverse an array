#include<stdio.h>
int main()
{
    int n,i;
    printf("enter the array size :");
    scanf("%d",&n);
    int a[n];
    printf("enter the elments of an array:");
    for(i=0;i<n;i++)
    {
        scanf("\n%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        printf("\n%d",a[i]);
    }
    for(i=n-1;i>=0;i--)
    {
        printf("%d",a[i]);
    }
    return 0;
}
