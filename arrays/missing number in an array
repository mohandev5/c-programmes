#include<stdio.h>
#include<math.h>
int main()
{
    int a[5]={4,9,25,36},i;
    int sum1=0;
    for(i=0;i<5;i++)
    {
        sum1=sum1+a[i];
    }
    int sum2=0;
    for(i=2;i<=6;i++)
    {
        sum2=sum2+pow(i,2);
    }
    int msg;
    msg=sum2-sum1;
    printf("missing number in an array is:%d",msg);
}
