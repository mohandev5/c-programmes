#include<stdio.h>
int main()
{
    int n,i,p;
    printf("enter the size of an array :");
    scanf("%d",&n);
    int a[n];
    printf("enter the elements of an arry:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("choose index you need to delete :");
    scanf("%d",&p);
    for(i=0;i<n;i++)
    {
        if(a[i]==a[p])
        {
            continue;
        }
        printf("%d\t",a[i]);
    }
}
