#include<stdio.h>
int main()
{
    int a[5]={1,2,3,4,5};
    int b[5]={1,2,3,4,5};
    int c[5],i;
    for(i=0;i<5;i++)
    {
        printf("%d \n",a[i]);
    }
    printf("\n");
    for(i=0;i<5;i++)
    {
        printf("%d \n",b[i]);
    }
    printf("\n");
    for(i=0;i<5;i++)
    {
        c[i]=a[i]+b[i];
        printf("sum of arrays :%d \n",c[i]);
    }
}
